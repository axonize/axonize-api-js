{
  "name": "@axonize/api",
  "version": "0.5.6",
  "private": false,
  "description": "Axonize API client library",
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
  "scripts": {
    "test": "jest -i --config jestconfig.json",
    "watch:test": "jest -i --config jestconfig.json --watch",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add .",
    "postversion": "git push --follow-tags"
  },
  "keywords": [
    "axonize",
    "api",
    "client"
  ],
  "author": "Shaked Amar",
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/nock": "^10.0.3",
    "@types/node-fetch": "^2.3.7",
    "dotenv": "^8.0.0",
    "jest": "^24.8.0",
    "nock": "^10.0.6",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "axios": "^0.19.0"
  }
}
