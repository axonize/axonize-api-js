{
  "transform": {
    "^.+\\.(t)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
  "moduleFileExtensions": ["ts", "js", "json", "node"],
  "setupFiles": ["dotenv/config"],
  "reporters": [ "default", ["jest-junit", {"jestJunitOutputDir": "test-results/jest" }] ]
}